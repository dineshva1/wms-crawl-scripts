# =============================================================================
# RZN1 WMS CRAWL SCRIPTS - ENVIRONMENT CONFIGURATION
# =============================================================================
# Copy this file to .env and replace the placeholder values with your actual 
# credentials and configuration details.
#
# SECURITY WARNING: Never commit the actual .env file to version control!
# =============================================================================

# -----------------------------------------------------------------------------
# RZN1 SERVICE CONFIGURATION
# -----------------------------------------------------------------------------
# Base configuration for RZN1 service authentication and API endpoints
RZN1_BASE_URL=https://rzn1-be.stockone.com
RZN1_AUTH_ENDPOINT=/o/token/
RZN1_CLIENT_ID=your-rzn1-client-id-here
RZN1_CLIENT_SECRET=your-rzn1-client-secret-here
RZN1_GET_REPORTS_ENDPOINT=/api/v1/reports/generatedReports/
RZN1_WAREHOUSE=up108_kum_ls1

# -----------------------------------------------------------------------------
# RZN1 API ENDPOINTS
# -----------------------------------------------------------------------------
# Specific endpoints for different report generation APIs
RZN1_ORDER_SUMMARY_ENDPOINT=/api/v1/reports/generate-report/
RZN1_SALES_RETURN_ENDPOINT=/api/v1/reports/generate-report/
RZN1_INVENTORY_SUMMARY_ENDPOINT=/api/v1/reports/generate-report/
RZN1_CLOSING_STOCK_ENDPOINT=/api/v1/reports/generate-report/

# -----------------------------------------------------------------------------
# FILE PROCESSING CONFIGURATION
# -----------------------------------------------------------------------------
# Base filenames for different report types (without date suffixes)
ORDER_SUMMARY_FILENAME=ORDER_SUMMARY
SALES_RETURNS_FILENAME=SALES_RETURN
BATCH_LEVEL_INVENTORY_FILENAME=BATCH_LEVEL_INVENTORY
OPEN_ORDER_SUMMARY_FILENAME=OPEN_ORDER_SUMMARY

# -----------------------------------------------------------------------------
# ORDER SUMMARY PROCESSOR CONFIGURATION
# -----------------------------------------------------------------------------
# S3 paths and output filenames for order summary workflow
ORDER_SUMMARY_INPUT_PREFIX=rzn1/order_summary/raw
ORDER_SUMMARY_OUTPUT_PREFIX=rzn1/order_summary/processed
ORDER_SUMMARY_MTD_PREFIX=rzn1/order_summary/report/main/sales

ORDER_OUTPUT_COMPLETE_FILENAME=ORDER_SUMMARY_COMPLETE
ORDER_OUTPUT_UP_FILENAME=ORDER_SUMMARY_UP
ORDER_OUTPUT_HR_FILENAME=ORDER_SUMMARY_HR

# -----------------------------------------------------------------------------
# INVENTORY SUMMARY PROCESSOR CONFIGURATION
# -----------------------------------------------------------------------------
# S3 paths and output filenames for inventory summary workflow
INVENTORY_SUMMARY_INPUT_PREFIX=rzn1/inventory_summary/raw
INVENTORY_SUMMARY_OUTPUT_PREFIX=rzn1/inventory_summary/processed
INVENTORY_OUTPUT_COMPLETE_FILENAME=INVENTORY_SUMMARY_COMPLETE

# -----------------------------------------------------------------------------
# CLOSING STOCK PROCESSOR CONFIGURATION
# -----------------------------------------------------------------------------
# S3 paths and output filenames for closing stock workflow
# Note: Closing stock uses inventory_summary folder structure
BATCH_LEVEL_CLOSINGSTOCK_FILENAME=CLOSING_STOCK
CLOSINGSTOCK_OUTPUT_UP_FILENAME=CLOSINGSTOCK_UP
CLOSINGSTOCK_OUTPUT_HR_FILENAME=CLOSINGSTOCK_HR
CLOSINGSTOCK_INPUT_PREFIX=rzn1/inventory_summary/raw
CLOSINGSTOCK_OUTPUT_PREFIX=rzn1/inventory_summary/processed

# Legacy closing stock configuration (if needed for backward compatibility)
CLOSING_STOCK_INPUT_PREFIX=rzn1/closing_stock/raw
CLOSING_STOCK_OUTPUT_PREFIX=rzn1/closing_stock/processed
CLOSING_STOCK_OUTPUT_COMPLETE_FILENAME=CLOSING_STOCK_COMPLETE

# -----------------------------------------------------------------------------
# AWS CONFIGURATION
# -----------------------------------------------------------------------------
# AWS credentials and S3 bucket configuration
# WARNING: Keep these credentials secure and never commit to version control
AWS_ACCESS_KEY_ID=your-aws-access-key-id-here
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key-here
AWS_REGION=ap-south-1
BUCKET_NAME=your-s3-bucket-name-here

# -----------------------------------------------------------------------------
# RZN SERVICE CONFIGURATION (OPTIONAL)
# -----------------------------------------------------------------------------
# Additional service credentials if needed for extended functionality
RZN_CLIENT_ID=your-rzn-client-id-here
RZN_CLIENT_SECRET=your-rzn-client-secret-here

# =============================================================================
# CONFIGURATION NOTES:
# =============================================================================
# 1. Replace all "your-*-here" placeholders with actual values
# 2. Ensure AWS IAM user has appropriate S3 permissions
# 3. Verify RZN1 service credentials and warehouse code
# 4. Test configuration with --skip-processors flag first
# 5. Monitor logs for any authentication or permission issues
# =============================================================================
